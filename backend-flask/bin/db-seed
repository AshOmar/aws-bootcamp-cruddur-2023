#! /usr/bin/bash

BRED='\033[1;31m'
NC='\033[0m' # No Color

echo -e "\n${BRED}=== DB Seed Start ===${NC}\n"

if [ "$1" == "prod" ]; then
    URL=$PROD_CONNECTION_URL
else
    URL=$CONNECTION_URL
fi

#echo $(realpath ..)
seed_path=$(realpath ..)/db/seed.sql
#echo $schema_path
#echo Running from $(realpath "$0")
psql $URL cruddur < $seed_path

echo -e "\n${BRED}=== DB Seed Finish ===${NC}\n"